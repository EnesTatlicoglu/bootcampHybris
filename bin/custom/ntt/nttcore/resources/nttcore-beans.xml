<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

    <bean class="de.hybris.platform.commerceservices.service.data.CommerceCartParameter">
        <property name="customerNote" type="String"/>
    </bean>
    
    <bean class="tr.nttdata.bootcamp.core.product.data.ProductSellData">
        <property name="productCode" type="java.lang.String"/>
        <property name="soldCount" type="java.lang.Long"/>
    </bean>

    <bean class="tr.nttdata.bootcamp.core.product.parameter.ProductQuestionParameter">
        <property name="product" type="de.hybris.platform.core.model.product.ProductModel"/>
        <property name="question" type="java.lang.String"/>
        <property name="category" type="tr.nttdata.bootcamp.core.model.ProductQuestionCategoryModel"/>
        <property name="hideUser" type="boolean"/>
        <property name="onlyForUser" type="boolean"/>
    </bean>
    
</beans>